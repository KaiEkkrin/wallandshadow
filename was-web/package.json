  "scripts": {
    "start": "run-p --race dev:firebase dev:vite",
    "dev:firebase": "IS_LOCAL_DEV=true FUNCTIONS_EMULATOR=true firebase emulators:start --only auth,firestore,functions,hosting,storage",
    "dev:vite": "vite --host",
    "typecheck": "tsc --noEmit",
    "build": "tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest --config ./unit/vitest.config.ts run",
    "test:unit": "vitest --config ./unit/vitest.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report --host 0.0.0.0 e2e/test-results/html",
    "lint": "eslint src"
  },
  "resolutions": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "jws": "^4.0.1"
  },
